					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Generate performance test summary",
									"const testEndTime = new Date().toISOString();",
									"const testRunId = pm.environment.get('testRunId');",
									"const testStartTime = pm.environment.get('testStartTime');",
									"",
