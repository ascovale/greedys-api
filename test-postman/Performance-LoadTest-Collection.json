			"description": "Initialize performance testing environment and variables",
			"item": [
				{
					"name": "Initialize Performance Test Run",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Initialize performance test run",
									"const testRunId = 'perf_' + new Date().getTime() + '_' + Math.random().toString(36).substr(2, 9);",
									"const testStartTime = new Date().toISOString();",
									"",
									"pm.environment.set('testRunId', testRunId);",
									"pm.environment.set('testStartTime', testStartTime);",
									"pm.environment.set('performanceMonitoring', 'true');",
									"",
									"console.log('üöÄ PERFORMANCE TEST INITIALIZATION:');",
									"console.log('  üìä Test Run ID:', testRunId);",
									"console.log('  üïí Start Time:', testStartTime);",
									"console.log('  üéØ Max Response Time:', pm.environment.get('maxResponseTime') + 'ms');",
									"console.log('  ‚ö° Fast Threshold:', pm.environment.get('fastResponseTime') + 'ms');",
									"console.log('  üêå Slow Threshold:', pm.environment.get('slowResponseTime') + 'ms');"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Performance test environment initialized\", function () {",
									"    pm.expect(pm.environment.get('testRunId')).to.not.be.empty;",
									"    pm.expect(pm.environment.get('testStartTime')).to.not.be.empty;",
									"});",
									"",
									"console.log('‚úÖ Performance test environment ready for execution');"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/health",
							"host": ["{{baseUrl}}"],
							"path": ["health"]
						}
					}
				}
			]
		},
		{
			"name": "Authentication Performance Tests",
			"description": "Test authentication endpoint performance under load",
			"item": [
				{
					"name": "Customer Login Performance Test",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Generate unique test customer for performance testing",
									"const timestamp = new Date().getTime();",
									"const perfTestEmail = `perf.customer.${timestamp}@greedy.test.it`;",
									"pm.environment.set('perfTestCustomerEmail', perfTestEmail);",
									"",
									"console.log('üèÉ CUSTOMER LOGIN PERFORMANCE TEST');",
									"console.log('  üë§ Test User:', perfTestEmail);",
									"console.log('  üéØ Target: < ' + pm.environment.get('fastResponseTime') + 'ms');"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"const responseTime = pm.response.responseTime;",
									"const maxTime = parseInt(pm.environment.get('maxResponseTime'));",
									"const fastTime = parseInt(pm.environment.get('fastResponseTime'));",
									"const slowTime = parseInt(pm.environment.get('slowResponseTime'));",
									"",
									"// Performance Test Assertions",
									"pm.test(`Response time is under maximum threshold (${maxTime}ms)`, function () {",
									"    pm.expect(responseTime).to.be.below(maxTime);",
									"});",
									"",
									"pm.test(\"Login successful\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"// Performance Classification",
									"if (responseTime < fastTime) {",
									"    pm.test(`‚ö° FAST LOGIN: ${responseTime}ms (< ${fastTime}ms)`, function () {",
									"        pm.expect(responseTime).to.be.below(fastTime);",
									"    });",
									"    console.log('‚ö° FAST LOGIN PERFORMANCE:', responseTime + 'ms');",
									"} else if (responseTime < slowTime) {",
									"    pm.test(`‚úÖ ACCEPTABLE LOGIN: ${responseTime}ms (< ${slowTime}ms)`, function () {",
									"        pm.expect(responseTime).to.be.below(slowTime);",
									"    });",
									"    console.log('‚úÖ ACCEPTABLE LOGIN PERFORMANCE:', responseTime + 'ms');",
									"} else {",
									"    pm.test(`üêå SLOW LOGIN: ${responseTime}ms (‚â• ${slowTime}ms)`, function () {",
									"        console.warn('‚ö†Ô∏è Login performance below expected threshold');",
									"        pm.expect(responseTime).to.be.below(maxTime);",
									"    });",
									"    console.log('üêå SLOW LOGIN PERFORMANCE:', responseTime + 'ms');",
									"}",
									"",
									"// Store token for subsequent performance tests",
									"if (pm.response.code === 200) {",
									"    const jsonData = pm.response.json();",
									"    if (jsonData && jsonData.data && jsonData.data.jwt) {",
									"        pm.environment.set('perfTestCustomerToken', jsonData.data.jwt);",
									"    }",
									"}",
									"",
									"// Performance Logging",
									"console.log('üìä CUSTOMER LOGIN PERFORMANCE METRICS:');",
									"console.log('  ‚è±Ô∏è  Response Time:', responseTime + 'ms');",
									"console.log('  üìè Response Size:', pm.response.responseSize || 'N/A');",
									"console.log('  üéØ Status Code:', pm.response.code);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "User-Agent",
								"value": "{{userAgent}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"{{customerEmail}}\",\n    \"password\": \"{{customerPassword}}\",\n    \"rememberMe\": true\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/customer/auth/login",
							"host": ["{{baseUrl}}"],
							"path": ["customer", "auth", "login"]
						}
					}
				},
