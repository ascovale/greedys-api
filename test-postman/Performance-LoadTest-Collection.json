		{
			"name": "Load Testing Scenarios",
			"description": "Simulate concurrent user load and stress testing scenarios",
			"item": [
				{
					"name": "Concurrent Login Load Test",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Simulate concurrent user by adding randomization",
									"const randomDelay = Math.random() * parseInt(pm.environment.get('requestDelay') || '100');",
									"",
									"// Add unique identifier for this concurrent request",
									"const concurrentId = 'concurrent_' + Math.random().toString(36).substr(2, 9);",
									"pm.environment.set('concurrentRequestId', concurrentId);",
									"",
									"console.log('üöÄ CONCURRENT LOGIN TEST - ID:', concurrentId);",
									"console.log('  ‚è∞ Randomized Delay:', Math.round(randomDelay) + 'ms');",
									"",
									"// Optional delay to simulate real user behavior",
									"if (randomDelay > 10) {",
									"    setTimeout(() => {}, randomDelay);",
									"}"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"const responseTime = pm.response.responseTime;",
									"const maxTime = parseInt(pm.environment.get('maxResponseTime'));",
									"const concurrentId = pm.environment.get('concurrentRequestId');",
									"",
									"pm.test(`Concurrent login ${concurrentId} completed`, function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(`Response time under load: ${responseTime}ms < ${maxTime}ms`, function () {",
									"    pm.expect(responseTime).to.be.below(maxTime);",
									"});",
									"",
									"// Log concurrent request performance",
									"console.log('üìä CONCURRENT REQUEST PERFORMANCE:');",
									"console.log('  üÜî Request ID:', concurrentId);",
									"console.log('  ‚è±Ô∏è  Response Time:', responseTime + 'ms');",
									"console.log('  üéØ Status:', pm.response.code);",
									"",
									"// Store performance data for analysis",
									"pm.globals.set(`perf_${concurrentId}`, JSON.stringify({",
									"    id: concurrentId,",
									"    responseTime: responseTime,",
									"    status: pm.response.code,",
									"    timestamp: new Date().toISOString()",
									"}));"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"{{customerEmail}}\",\n    \"password\": \"{{customerPassword}}\",\n    \"rememberMe\": true\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/customer/auth/login",
							"host": ["{{baseUrl}}"],
							"path": ["customer", "auth", "login"]
						}
					}
				},
