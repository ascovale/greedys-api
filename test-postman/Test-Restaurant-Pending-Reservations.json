			"name": "Restaurant Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 201) {",
							"    const responseJson = pm.response.json();",
							"    if (responseJson.success && responseJson.data && responseJson.data.accessToken) {",
							"        pm.environment.set('restaurantToken', responseJson.data.accessToken);",
							"        console.log('‚úÖ Restaurant login successful, token saved');",
							"    } else {",
							"        console.log('‚ùå Login response missing token data');",
							"    }",
							"} else {",
							"    console.log('‚ùå Restaurant login failed with status:', pm.response.code);",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"email\": \"ascolesevalentino@gmail.com\",\n    \"password\": \"Minosse100%\"\n}"
				},
				"url": {
					"raw": "{{baseUrl}}/restaurant/user/auth/authenticate",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"restaurant",
						"user",
						"auth",
						"authenticate"
					]
				}
			},
			"response": []
		},
