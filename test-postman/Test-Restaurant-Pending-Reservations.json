		{
			"name": "Get All Reservations - For Comparison",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"console.log('Response Status:', pm.response.code);",
							"",
							"if (pm.response.code === 200) {",
							"    const responseJson = pm.response.json();",
							"    console.log('‚úÖ All reservations - Success!');",
							"    console.log('Total reservations found:', responseJson.data ? responseJson.data.length : 0);",
							"    if (responseJson.data && responseJson.data.length > 0) {",
							"        console.log('Reservations by status:');",
							"        const statusCounts = {};",
							"        responseJson.data.forEach(reservation => {",
							"            statusCounts[reservation.status] = (statusCounts[reservation.status] || 0) + 1;",
							"            if (reservation.status === 'NOT_ACCEPTED') {",
							"                console.log(`  üìã Pending: ID=${reservation.id}, Date=${reservation.date}, Customer=${reservation.customerName || 'N/A'}`);",
							"            }",
							"        });",
							"        Object.keys(statusCounts).forEach(status => {",
							"            console.log(`  ${status}: ${statusCounts[status]} reservations`);",
							"        });",
							"    }",
							"} else {",
							"    console.log('‚ùå All reservations - Failed with status:', pm.response.code);",
							"}"
						],
